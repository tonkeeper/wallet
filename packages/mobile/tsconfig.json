{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "useUnknownInCatchVariables": false,
    "allowJs": true,
    "skipLibCheck": false,
    "strict": true,
    "noImplicitAny": false,
    "baseUrl": "src",
    "paths": {
      "$api": ["api"],
      "$core": ["core"],
      "$modals": ["modals"],
      "$store": ["store"],
      "$uikit": ["uikit"],
      "$utils": ["utils"],
      "$hooks": ["hooks"],
      "$libs": ["libs"],
      "$styled": ["styled"],
      "$styles": ["styles"],
      "$shared": ["shared"],
      "$assets": ["assets"],
      "$navigation": ["navigation"],
      "$wallet": ["wallet"],
      "$translation": ["translation"],
      "$tonconnect": ["tonconnect"],
      "$api/*": ["api/*"],
      "$core/*": ["core/*"],
      "$modals/*": ["modals/*"],
      "$store/*": ["store/*"],
      "$uikit/*": ["uikit/*"],
      "$utils/*": ["utils/*"],
      "$hooks/*": ["hooks/*"],
      "$libs/*": ["libs/*"],
      "$styled/*": ["styled/*"],
      "$styles/*": ["styles/*"],
      "$shared/*": ["shared/*"],
      "$assets/*": ["assets/*"],
      "$navigation/*": ["navigation/*"],
      "$wallet/*": ["wallet/*"],
      "$translation/*": ["translation/"],
      "$blockchain": ["blockchain/"],
      "$database": ["database/"],
      "$tonconnect/*": ["tonconnect/*"],
      "$config": ["config/"]
    }
  },
  "include": ["src", "src/wallet/managers", "src/wallet/Wallet"],
  "exclude": ["node_modules"]
}